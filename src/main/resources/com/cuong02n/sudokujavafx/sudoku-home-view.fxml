<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.cuong02n.sudokujavafx.SudokuHomeController" stylesheets="@css/style.css">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <Button text="Play" fx:id="playButton" styleClass="button"/>
    <Button text="Highscore" fx:id="highscoreButton" styleClass="button"/>
    <Button text="Settings" fx:id="settingsButton" styleClass="button"/>
    <Button text="Exit" fx:id="exitButton" styleClass="button"/>

</VBox>
